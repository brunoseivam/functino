record(ai, "$(NAME)-I") {
    field(DESC, "Get $(DESC)")
    field(DTYP, "stream")
    field(INP,  "@functino.proto read($(CMD)) $(PORT)")
    field(SCAN, "$(SCAN=1 second)")
    field(EGU,  "$(EGU=)")
}

record(ao, "$(NAME)-SP") {
    field(DESC, "Set $(DESC)")
    field(DTYP, "stream")
    field(OUT,  "@functino.proto write($(CMD)) $(PORT)")
    field(EGU,  "$(EGU=)")
    field(FLNK, "$(NAME)-I")
}
